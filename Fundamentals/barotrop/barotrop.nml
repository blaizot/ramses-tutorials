This is the RAMSES parameter file for the barotrop EOS test.
It consists of a collapsing uniform sphere with central overdensity.

&RUN_PARAMS
hydro=.true.
poisson=.true.
nsubcycle=20*1
ncontrol=50
/

&AMR_PARAMS
levelmin=5
levelmax=16
ngridmax=5000
boxlen=1.
nexpand=20*10
/

&REFINE_PARAMS
interpol_var=1
interpol_type=0
jeans_refine=20*20
err_grad_d=1d3
/

&HYDRO_PARAMS
gamma=1.666667
courant_factor=0.8
scheme='muscl'
slope_type=1
riemann='hllc'
pressure_fix=.true.
beta_fix=0.5
/

&INIT_PARAMS
nregion=2
region_type(1)='square'
region_type(2)='square'
x_center=0.5,0.5
length_x=1.,0.01
exp_region=10,10
d_region=66.44829213565784,664482.9213565784
p_region=2.418894075235192,24188.94075235192
/

&OUTPUT_PARAMS
tend=0.00049
/

&UNITS_PARAMS
units_density=1.50492957435d-20
units_time=3.1556926d13
units_length=3.0857d18
/

&COOLING_PARAMS
barotropic_eos=.true.
barotropic_eos_form='double_polytrope'
polytrope_rho=1d-15 !g/cm3
polytrope_index=1.666667
T_eos=10d0
mu_gas=2.37d0
/
